{
  "name": "api-cafeto",
  "version": "1.0.0",
  "description": "Api para manejar eventos y usuarios",
  "scripts": {
    "build": "gulp server:build",
    "start": "node build/bin/www.js",
    "start:dev": "DEBUG=api-cafeto:* gulp server:dev",
    "start:dev:watch": "DEBUG=api-cafeto:* gulp server:watch",
    "start:dev:debug": "npm run build && node --inspect build/bin/www.js",
    "test": "jest --config jest.config.json --detectOpenHandles",
    "test:watch": "DEBUG=test:* npm test -- --verbose --watch",
    "release": "standard-version -a",
    "lint": "npm run lint:eslint -- --ext .ts src/",
    "lint:eslint": "eslint --ignore-path .eslintignore",
    "lint:fix": "npm run lint -- --fix --cache",
    "prettier": "prettier --config .prettierrc.json --ignore-path ./.prettierignore --write src/**/*.ts",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Alver23/api-cafeto.git"
  },
  "keywords": [
    "Api",
    "Cafeto",
    "eventos",
    "usuarios"
  ],
  "author": "Alver Grisales <viga.23@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alver23/api-cafeto/issues"
  },
  "homepage": "https://github.com/Alver23/api-cafeto#readme",
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "@hapi/joi": "^17.1.1",
    "@sentry/node": "^5.15.5",
    "@types/jsonwebtoken": "^8.5.0",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.21.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-healthcheck": "^0.1.0",
    "helmet": "^3.22.0",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.9",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.31",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/hapi__joi": "^17.1.0",
    "@types/jest": "^25.2.2",
    "@types/method-override": "0.0.31",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.1",
    "@types/passport": "^1.0.3",
    "@types/passport-http": "^0.3.8",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.9",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "coveralls": "^3.1.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "gulp": "^4.0.2",
    "gulp-nodemon": "^2.5.0",
    "gulp-shell": "^0.8.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.2.5",
    "jest": "~24.9.0",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "~24.0.2",
    "typescript": "^3.9.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,json}": [
      "prettier --write --config .prettierrc.json"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
